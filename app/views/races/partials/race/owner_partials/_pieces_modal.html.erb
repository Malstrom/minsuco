<!-- Modal-->
<div id="piecesModal_<%= attendee.id %>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" class="modal fade">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">

      <div class="modal-header">
        <button id="closeMyModal" type="button" data-dismiss="modal" aria-label="Close" class="close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h4 id="myModalLabel" class="modal-title">Provvigioni da pagare<small class="text-muted"></small></h4>
      </div>

      <div class="panel-body">
        <div></div>
        <%= render partial: "races/partials/revenue_table", :locals => { user: attendee.user }  %>

      </div>

    </div>
  </div>
</div>